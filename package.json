{
	"name": "svelte-monaco",
	"description": "Monaco Editor bindings for Svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit sync && svelte-package && publint",
		"preview": "vite preview",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"author": {
		"name": "Tristan F.",
		"email": "leodog896@gmail.com"
	},
	"repository": "github:LeoDog896/svelte-monaco",
	"homepage": "https://leodog896.github.io/svelte-monaco",
	"bugs": {
		"url": "https://github.com/LeoDog896/svelte-monaco/issues",
		"email": "leodog896@gmail.com"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"devDependencies": {
		"@monaco-editor/loader": "^1.3.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.22.3",
		"@sveltejs/package": "^2.2.0",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"monaco-editor": "^0.41.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.16",
		"svelte": "^4.1.1",
		"svelte-check": "^3.4.6",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7"
	},
	"type": "module",
	"dependencies": {
		"monaco-themes": "^0.4.4"
	},
	"peerDependencies": {
		"monaco-editor": ">=0.40.0 < 1",
		"svelte": ">=4.0.0"
	}
}
